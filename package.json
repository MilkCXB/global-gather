{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "preexport": "npm run build",
    "export": "next export",
    "build": "yarn compile-external-abi-types && next build && next export",
    "start": "next start",
    "lint": "next lint",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "extract": "NODE_ENV=development lingui extract",
    "extract:en": "NODE_ENV=development lingui extract --locale en-US",
    "compile": "lingui compile",
    "compile-external-abi-types": "typechain --target ethers-v5 --out-dir src/abis/types './src/abis/**/*.json'"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@lingui/react": "^3.13.2",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "ethers": "^5.5.4",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "lodash": "^4.17.21",
    "lqip-loader": "^2.2.1",
    "next": "latest",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "react": "latest",
    "react-circle": "^1.1.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "latest",
    "react-helmet-async": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@iconify/react": "^3.1.3",
    "@lingui/cli": "^3.13.2",
    "@lingui/loader": "^3.13.2",
    "@lingui/macro": "^3.13.2",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "latest",
    "@types/react": "latest",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.4.0",
    "typechain": "^7.0.0",
    "typescript": "latest"
  }
}
